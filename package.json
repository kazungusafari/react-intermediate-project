{
  "name": "react-intermediate-project",
  "version": "1.0.0",
  "description": "Project to be used to learn react intermediate concepts",
  "main": "src/App.js",
  "dependencies": {
    "@babel/node": "^7.12.10",
    "@emotion/babel-preset-css-prop": "^11.0.0",
    "@emotion/core": "^11.0.0",
    "@emotion/css": "^11.1.3",
    "@emotion/react": "^11.1.4",
    "@frontendmasters/pet": "^1.0.3",
    "@reach/router": "^1.2.1",
    "babel-cli": "^6.26.0",
    "express": "^4.17.1",
    "react": "^17.0.1",
    "react-dom": "^16.8.4"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "@testing-library/react": "^11.2.3",
    "babel-eslint": "^10.0.1",
    "babel-plugin-emotion": "^11.0.0",
    "babel-plugin-jsx-pragmatic": "^1.0.2",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "eslint": "^5.12.1",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-import": "^2.15.0",
    "eslint-plugin-jsx-a11y": "^6.2.0",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.0.2",
    "jest": "^26.6.3",
    "parcel-bundler": "^1.12.1",
    "prettier": "^1.16.1"
  },
  "scripts": {
    "build": "parcel build --public-url ./dist/ src/index.html",
    "clear-build-cache": "rm -rf .cache/ dist/",
    "dev": "parcel src/index.html",
    "format": "prettier --write \"src/**/*.{js,jsx}\"",
    "lint": "eslint \"src/**/*.{js,jsx}\" --quiet",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:update": "jest -u",
    "start": "npm run build && babel-node server/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kazungusafari/react-intermediate-project.git"
  },
  "keywords": [],
